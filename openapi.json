                  "success": true,
                  "expensePreview": {
                      "month": {
                          "_id": "month",
                          "totalSpent": 3400000
                      },
                      "today": {
                          "_id": "today",
                          "totalSpent": 89700
                      },
                      "yesterday": {
                        "_id": "yesterday",
                        "totalSpent": 3400
