            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Custom Error"
                },
                "example": {
                  "success": false,
                  "status": 400,
                  "failedOperation": "operation-name",
                  "message": "error message",
                  "args": []
                  }
              }
            }
        },
        "Content Header Error": {
